                                {{lang('signin.remember_me')}}
                                <input type="checkbox" name="LoginForm[remember]" value="1" id="remember">
                            </label>
                        </div>
                    </div>
                    {% if captcha %}
                        <div class="g-recaptcha form-group" data-sitekey="{{captchaKey}}"></div>
                    {% endif %}
                    <input type="hidden" name="_csrf" value="{{csrftoken}}">
                    <div class="flex-btn-row">
                        {% if site['resetPassword'] %}
                            <a href="{{ page_url('resetpassword') }}" class="forgot-password1">{{ lang('signin.reset_password') }}</a>
                        {% endif %}
                        <button type="submit" class="btn btn-primary">{{ lang('signin.button') }}</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


