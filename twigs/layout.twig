                        "<div class='col-sm-4'><div class='update-box'>" + "<div class='row'><div class='col-xs-3'><h4 class='update-title'>ID</h4><p>" + data.linked_service.service + "</p></div>" + "<div class='col-xs-9'><h4 class='update-title'>DATE</h4><p>" + data.created_at + "</p></div></div>" + "<div class='row servRow'><div class='col-sm-12'><h4 class='update-title'>SERVICE</h4><p>" + data.linked_service.name + "</p></div></div>" + "<div class='row'><div class='col-sm-12'><div class='form-group ig3 " + type + "'><label>UPDATE</label><input class='form-control' type='text' value='" + data.update + "' readonly /></div></div></div>" + "</div></div>"
                    );
                });

                populatePaginaiton(response);
            });
        }

